<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="false" enabled="true" name="AM-EncodeIssuerB64">
    <DisplayName>AM-EncodeIssuerB64</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>issuer-encoded</Name>
        <Template>{encodeBase64(issuer)}</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>