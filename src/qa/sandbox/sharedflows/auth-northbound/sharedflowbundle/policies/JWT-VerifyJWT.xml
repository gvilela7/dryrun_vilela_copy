<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<VerifyJWT continueOnError="true" enabled="true" name="JWT-VerifyJWT">
    <DisplayName>JWT-VerifyJWT</DisplayName>
    <Algorithm>RS256</Algorithm>
    <Source>private.client.token</Source>
    <PublicKey>
        <Value ref="publicKey-decoded"/>
    </PublicKey>
    <Audience>agite-apigee</Audience>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
</VerifyJWT>