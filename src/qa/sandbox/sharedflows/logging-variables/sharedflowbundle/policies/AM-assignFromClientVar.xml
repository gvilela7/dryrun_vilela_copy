<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="true" enabled="true" name="AM-assignFromClientVar">
    <DisplayName>AM-assignFromClientVar</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>fromClient.uri</Name>
        <Ref>request.uri</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>fromClient.body</Name>
        <Ref>request.content</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>fromClient.bodysize</Name>
        <Ref>request.header.content-length</Ref>
        <Value>0</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>fromClient.verb</Name>
        <Ref>request.verb</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>fromClient.querystring</Name>
        <Ref>request.querystring</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>fromClient.headers</Name>
        <Ref>jsreqheaders</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>logState</Name>
        <Value>fromClient</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>